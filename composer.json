{
  "name": "sk-id-solutions/mobile-id-php-client",
  "type": "library",
  "description": "Mobile-ID Relying Party PHP Api vclient",
  "license": "MIT",

  "require": {
    "php": "^7.0",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "hrobertson/x509-verify": "dev-phpv7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.0"
  },
  "autoload": {
    "psr-4": {
      "Sk\\Mid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sk\\Mid\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "url": "https://github.com/AndreiJantsuk/php-x509-verify.git",
      "type": "git"
    }
  ]
}

